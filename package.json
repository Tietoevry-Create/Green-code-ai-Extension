{
  "name": "green-code-ai",
  "displayName": "Green Code AI",
  "publisher": "Green Coding",
  "description": "Code review for sustainability and green code practices",
  "version": "0.0.1",
  "icon": "static/GC-logo.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/tietoevryCreate/Green-code-ai-Extension.git"
  },
  "main": "./out/main.js",
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf out"
  },
  "contributes": {
    "commands": [
      {
        "command": "greenCoding.start",
        "title": "Start Green Coding",
        "category": "Green Code AI"
      },
      {
        "command": "your_extension_id.postMessage",
        "title": "Post Message Command"
      },
      {
        "command": "your_extension_id.aiResponse",
        "title": "AI Response Command"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@types/vscode": "^1.83.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.6",
    "@pinecone-database/pinecone": "^1.1.1",
    "axios": "^1.5.1",
    "chroma": "^0.0.1",
    "chromadb": "^1.5.11",
    "dotenv": "^16.3.1",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.170",
    "lib": "^5.1.0",
    "natural": "^6.8.0",
    "node-nlp": "^4.27.0",
    "openai": "^4.12.1",
    "pinecone": "^0.1.0",
    "svelte": "^4.2.2"
  }
}
