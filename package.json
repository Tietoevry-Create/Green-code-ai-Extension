{
  "name": "green-code-ai",
  "displayName": "Green Code AI",
  "publisher": "Green Coding",
  "description": "Code review for sustainability and green code practices",
  "version": "0.0.1",
  "icon": "static/GC-logo.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Geirtieto/Green-code-ai-Extension.git"
  },
  "main": "./out/extension.js",
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf out"
  },
  "contributes": {
    "commands": [
      {
        "command": "greenCoding.start",
        "title": "Start Green Coding",
        "category": "Green Code AI"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@types/vscode": "^1.83.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "openai": "^4.12.1"
  }
}
